  "scripts": {
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "client:mac":"cd frontend && npm start:mac",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start:mac":"concurrently --kill-others-on-fail \"npm run server\" \"npm run client:mac\""
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.9.1",
    "nodemon": "^2.0.22",
    "swagger-ui-express": "^4.6.2"
  }
}
